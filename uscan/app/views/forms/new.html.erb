<h1>Contact Us Page</h1>

<%= form_for :forms, url: forms_path do |f| %>
  <p>
    <%= f.label :first_name %><br>
    <%= f.text_field :first %>
  </p>
    <p>
    <%= f.label :last_name %><br>
    <%= f.text_field :last %>
  </p>
 
   <p>
    <%= f.label :email %><br>
    <%= f.text_field :email %>
  </p>
 
 
  <p>
    <%= f.label :message %><br>
    <%= f.text_area :message %>
  </p>
 
  <p>
    <%= f.submit %>
  </p>
<% end %>